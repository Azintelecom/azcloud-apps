AZCLOUD_APPS_KEYCLOAK_DOCKER_NETWORK="keycloak-network"
AZCLOUD_APPS_KEYCLOAK_DOCKER_DATABASES=(postgres mysql)
AZCLOUD_APPS_KEYCLOAK_DOCKER_DATABASE="postgres"
AZCLOUD_APPS_KEYCLOAK_DATABASE_NAME="keycloak"
AZCLOUD_APPS_KEYCLOAK_DATABASE_USER="keycloak"
AZCLOUD_APPS_KEYCLOAK_DATABASE_PASSWORD="keycloak"
AZCLOUD_APPS_KEYCLOAK_VERSION="latest"
AZCLOUD_APPS_KEYCLOAK_DOCKER_IMAGE="jboss/keycloak"
AZCLOUD_APPS_KEYCLOAK_NAME="keycloak"
AZCLOUD_APPS_KEYCLOAK_USER="admin"
AZCLOUD_APPS_KEYCLOAK_PASSWORD="admin"
AZCLOUD_APPS_KEYCLOAK_PROXY_ADDRESS_FORWARDING=true
AZCLOUD_APPS_KEYCLOAK_PROXY_IMAGE="nginx"
AZCLOUD_APPS_KEYCLOAK_PROXY_NAME="${AZCLOUD_APPS_KEYCLOAK_PROXY_IMAGE}"
AZCLOUD_APPS_KEYCLOAK_PROXY_PORTS="443:443"
AZCLOUD_APPS_KEYCLOAK_PROXY_CONFIG_DIR="/etc/${AZCLOUD_APPS_KEYCLOAK_PROXY_NAME}/conf.d"
AZCLOUD_APPS_KEYCLOAK_PROXY_CONFIG_FILE="${AZCLOUD_APPS_KEYCLOAK_PROXY_CONFIG_DIR}/keycloak.conf"
AZCLOUD_APPS_KEYCLOAK_PROXY_PROTO="https"
AZCLOUD_APPS_KEYCLOAK_CERTIFICATE_SELFSIGNED=true
AZCLOUD_APPS_KEYCLOAK_CERTIFICATE_DIR="/etc/${AZCLOUD_APPS_KEYCLOAK_PROXY_NAME}/certs"
AZCLOUD_APPS_KEYCLOAK_CERTIFICATE_FILE="${AZCLOUD_APPS_KEYCLOAK_CERTIFICATE_DIR}/cert.pem
AZCLOUD_APPS_KEYCLOAK_CERTIFICATE_KEY="${AZCLOUD_APPS_KEYCLOAK_CERTIFICATE_DIR}/key.pem"
AZCLOUD_APPS_KEYCLOAK_CERTIFICATE_SUBJECT="/C=AZ/ST=Absheron/L=Baku/O=AzinTelecom/CN=$HOSTNAME"
AZCLOUD_APPS_KEYCLOAK_CERTIFICATE_KEY_TYPE="rsa:4096"
AZCLOUD_APPS_KEYCLOAK_CERTIFICATE_DURATION="3650"
